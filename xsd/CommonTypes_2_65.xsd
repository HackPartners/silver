    <xs:complexType name="FulfillmentPhoneNumberType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="callingCountryCode" type="xs:integer"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
	
	<xs:complexType name="ConfirmationInformationRequiredOverrideType">
		<xs:sequence>
			<xs:element name="ticketingOptionCode" type="com:TicketingCodeConfirmationRequiredOverride" minOccurs="0" maxOccurs="unbounded"/>
