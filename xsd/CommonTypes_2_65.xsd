   		</xs:sequence>
   	</xs:complexType>
    <xs:complexType name="TravelPointPairDateType">
        <xs:complexContent>
            <xs:extension base="com:BaseTravelPointPairType">
                <xs:sequence>
              			<xs:element name="startDate" type="xs:date"/>
              			<xs:element name="endDate" type="xs:date" minOccurs="0"/>
              		</xs:sequence>
            </xs:extension>
        </xs:complexContent>
   	</xs:complexType>
	<xs:complexType name="TravelPointType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="type" type="com:TravelPointTypeEnumeration" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="TravelSegmentType">
		<xs:sequence>
			<xs:element name="originTravelPoint" type="com:TravelPointType"/>
			<xs:element name="destinationTravelPoint" type="com:TravelPointType"/>
			<xs:element name="departureDateTime" type="xs:dateTime"/>
			<xs:element name="arrivalDateTime" type="xs:dateTime"/>
			<xs:element name="serviceOrigins" type="com:SegmentServiceListType" minOccurs="0"/>
			<xs:element name="serviceDestinations" type="com:SegmentServiceListType" minOccurs="0"/>
			<xs:element name="designator" type="xs:string" minOccurs="0"/>
			<xs:element name="marketingCarrier" type="com:CarrierType" minOccurs="0"/>
			<xs:element name="operatingCarrier" type="com:CarrierType" minOccurs="0"/>
            <xs:element name="supplierEquipmentType" type="com:SupplierEquipmentTypeType" minOccurs="0"/>
			<xs:element name="distance" type="com:DistanceType" minOccurs="0"/>
			<xs:element name="duration" type="xs:duration" minOccurs="0"/>
			<xs:element name="onTimePercentage" type="xs:double" minOccurs="0"/>
			<xs:element name="amenities" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="amenity" type="com:AmenityType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="crossBorderInfo" type="xs:boolean" minOccurs="0"/>
			<xs:element name="supplierRecordLocator" type="com:AnorakRecordLocatorType"
				minOccurs="0"/>
			<xs:element name="equipmentType" type="com:EquipmentTypeType" minOccurs="0"/>
			<xs:element name="carbonOffset" type="xs:double" minOccurs="0"/>
			<xs:element name="viaTravelPoints" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="viaTravelPoint" type="com:TravelPointType"
							maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="marketingServiceName" type="xs:string" minOccurs="0"/>
            <xs:element name="marketingInformation" type="com:MarketingInformationType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="sequence">
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:minInclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="travelSegmentID" type="xs:ID" use="required"/>
		<xs:attribute name="type" type="xs:string"/>
		<xs:attribute name="legGrouping" type="xs:string" use="optional"/>
	</xs:complexType>
    <xs:complexType name="MarketingInformationType">
        <xs:sequence minOccurs="0" maxOccurs="1"/>
        <xs:attribute name="serviceCode" use="required"/>
    </xs:complexType>
	<xs:complexType name="TravelDocumentType">
		<xs:sequence>
			<xs:element name="documentNumber" type="xs:string"/>
			<xs:element name="documentType">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PHOTOCARD"/>
						<xs:enumeration value="PERMRESIDENT"/>
						<xs:enumeration value="PASSPORT"/>
						<xs:enumeration value="NATURALCERT"/>
						<xs:enumeration value="MILORDER"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="expirationDate" type="xs:date"/>
			<xs:element name="issuingAuthority">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="countryCode" type="com:ISOCountryCodeType"/>
						<xs:element name="stateProv" type="com:StateProvType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="documentAffiliation" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="CITIZENSHIP"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="TravelPointTypeEnumeration">
		<xs:restriction base="xs:string">
			<xs:enumeration value="STATION"/>
			<xs:enumeration value="CITY"/>
			<xs:enumeration value="REGION"/>
			<xs:enumeration value="COUNTRY"/>
		</xs:restriction>
	</xs:simpleType>
    <xs:complexType name="ValueDocumentCouponType">
        <xs:complexContent>
            <xs:extension base="com:CouponType">
                <xs:sequence>
                    <xs:element name="couponReceivedDate" type="xs:date" minOccurs="0"/>      <!-- Returned Date-->
                    <xs:element name="origin" type="com:TravelPointType" minOccurs="0"/>      <!-- Coupon Origin -->
                    <xs:element name="destination" type="com:TravelPointType" minOccurs="0"/> <!-- Coupon Destination -->
                    <xs:element name="refundApprovedDate" type="xs:date" minOccurs="0"/>      <!-- Approval for Refund Date -->
                    <xs:element name="authorization" minOccurs="0">                           <!-- Authorization Description and Code -->
                        <xs:complexType>
                            <xs:simpleContent>
                                <xs:extension base="xs:string">
                                    <xs:attribute name="code" type="xs:string"/>
                                </xs:extension>
                            </xs:simpleContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="departureDateTime" type="xs:dateTime" minOccurs="0" />
                    <xs:element name="ticketTypeCode" type="xs:string" minOccurs="0" />
                    <xs:element name="ticketType" type="xs:string" minOccurs="0" />
                    <xs:element name="numberAdults" type="xs:string" minOccurs="0" />
                    <xs:element name="numberChildren" type="xs:string" minOccurs="0" />
                    <xs:element name="railcardPrintingName" type="xs:string" minOccurs="0" />
					<xs:element name="ticketableFareIDRef" type="xs:IDREF" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="type">                                                    <!-- Coupon Number -->
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="SEAT"/>
							<xs:enumeration value="TICKET"/>
							<xs:enumeration value="TICKETED_RES"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	<xs:complexType name="ValueDocumentType">
		<xs:sequence>
			<xs:element name="valueDocumentLocator" type="com:AnorakRecordLocatorType"/>
			<xs:element name="retrievalCustomer" type="xs:string"/>
			<xs:element name="retrievalMaskedCreditCard" type="xs:string"/>
            <xs:element name="retrievalCode" type="xs:string"/>
            <xs:element name="coupons" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="coupon" type="com:ValueDocumentCouponType"
                                    maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
			<xs:element name="url" minOccurs="0" type="xs:string"/>
            <xs:element name="selectedTicketLanguage" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="issuedDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
            <xs:element name="data" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:base64Binary">
                            <xs:attribute name="type" type="xs:string" use="required"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
		</xs:sequence>
		<xs:attribute name="status" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="READY"/>
					<xs:enumeration value="CANCELED"/>
					<xs:enumeration value="PROCESSED"/>
					<xs:enumeration value="ISSUED"/>
					<xs:enumeration value="RETURNED"/>
                    <xs:enumeration value="RETURNED_PARTIAL"/>
					<xs:enumeration value="CONSUMED"/>
                    <xs:enumeration value="PRINTING"/>
                    <xs:enumeration value="FAILED"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="VoucherType">
		<xs:sequence>
			<xs:element name="number" type="xs:string"/>
            <xs:element name="validationCode" type="xs:string" minOccurs="0"/>
			<xs:element name="recipientName" type="xs:string"/>
			<xs:element name="expirationDate" type="xs:date"/>
            <xs:element name="value" type="com:MoneyType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="YesNoType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="YES"/>
			<xs:enumeration value="NO"/>
		</xs:restriction>
	</xs:simpleType>
    <xs:complexType name="PassengerInformationType">
        <xs:sequence>
            <xs:element name="acceptableFormOfTravelDocument" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        PERMRESIDENT: Legal Permanent Resident Card
                        PASSPORT: Passport
                        NATURALCERT: Naturalization Certificate
                        MILORDER: Military Order
                        PHOTOCARD: Photocard required in the UK for acquiring season tickets

                        Consult the SilverRail Documentation Portal for further information.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="PERMRESIDENT"/>
                        <xs:enumeration value="PASSPORT"/>
                        <xs:enumeration value="NATURALCERT"/>
                        <xs:enumeration value="MILORDER"/>
                        <xs:enumeration value="PHOTOCARD"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="type" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="PASSENGER_ADDRESS"/>
                    <xs:enumeration value="TRAVEL_DOCUMENTS"/>
                    <xs:enumeration value="DATE_OF_BIRTH"/>
                    <xs:enumeration value="GENDER"/>
                    <xs:enumeration value="RESIDENCE_INFORMATION"/>
		    <xs:enumeration value="PASSENGER_NAME"/>
		    <xs:enumeration value="PASSENGER_TITLE"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="allPassengers" use="required" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="TypesType">
        <xs:sequence>
            <xs:element name="type" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PaymentRequirementsType">
        <xs:sequence>
            <xs:element name="acceptableFormsOfPayment" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="formOfPayment" type="com:FormOfPaymentType"
                            maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="confirmationUponAddPayment" minOccurs="1" maxOccurs="1"
                type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="BaseTransactionFeeType" abstract="true">
		<xs:sequence>
			<xs:element name="fee" type="com:MoneyType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="transactionFeeID" type="xs:ID" use="required"/>
	</xs:complexType>


    	<xs:complexType name="ConfirmationOptionsType">
		<xs:sequence>
			<xs:element name="creditCardOption" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="types" type="com:TypesType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="debitCardOption" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="types" type="com:TypesType"/>
						<xs:element name="sortCodeRequired" type="xs:boolean"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="loyaltyCardOption" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="programs">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="program" type="xs:string" minOccurs="1"
										maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="expirationDateRequired" type="xs:boolean"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="personalInformationCardOption" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="countryCodes">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="countryCode" type="xs:string" minOccurs="1"
										maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
