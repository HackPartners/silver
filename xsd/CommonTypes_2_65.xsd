<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:com="http://railgds.net/ws/commontypes"
	targetNamespace="http://railgds.net/ws/commontypes" elementFormDefault="qualified"
	attributeFormDefault="unqualified" version="2.65">
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element name="company" type="xs:string" minOccurs="0"/>
			<xs:element name="address1" type="xs:string"/>
			<xs:element name="address2" type="xs:string" minOccurs="0"/>
			<xs:element name="city" type="xs:string"/>
			<xs:element name="stateProv" type="com:StateProvType" minOccurs="0"/>
			<xs:element name="zipCode" type="xs:string" minOccurs="0"/>
			<xs:element name="country" type="com:ISOCountryCodeType"/>
            <xs:element name="country3" type="com:ISOCountryCode3Type" minOccurs="0"/>
            <xs:element name="email" type="xs:string" minOccurs="0"/>
			<xs:element name="addressType">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="HOME"/>
						<xs:enumeration value="BUSINESS"/>
						<xs:enumeration value="UNKNOWN"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="AgencyCodeType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[a-zA-Z0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AmenityType">
      <xs:simpleContent>
          <xs:extension base="xs:string">
              <xs:attribute name="type" use="required">
                  <xs:simpleType>
                      <xs:restriction base="xs:string">
						<xs:enumeration value="BAGGAGE"/>
						<xs:enumeration value="BAR"/>
						<xs:enumeration value="BUFFET"/>
						<xs:enumeration value="DINING"/>
						<xs:enumeration value="HOT_FOOD"/>
						<xs:enumeration value="LOUNGE"/>
						<xs:enumeration value="MEAL"/>
						<xs:enumeration value="NETWORK"/>
						<xs:enumeration value="NEWSPAPER"/>
						<xs:enumeration value="QUIET"/>
						<xs:enumeration value="SNACK"/>
						<xs:enumeration value="TAXI"/>
						<xs:enumeration value="TROLLEY"/>
						<xs:enumeration value="WHEELCHAIR"/>
						<xs:enumeration value="WHEELCHAIR_RES"/>
						<xs:enumeration value="AUDIO"/>
						<xs:enumeration value="VIDEO"/>
						<xs:enumeration value="POWER_SOCKET"/>
						<xs:enumeration value="LOUNGE_ACCESS"/>
						<xs:enumeration value="INTERNET"/>
						<xs:enumeration value="INTERNET_AVAILABLE"/>
						<xs:enumeration value="PARKING"/>
					  </xs:restriction>
                  </xs:simpleType>
              </xs:attribute>
          </xs:extension>
      </xs:simpleContent>
  </xs:complexType>

	<xs:simpleType name="AnorakRecordLocatorType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="AnorakReceiptLocatorType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="BaseCreditCardType" abstract="true">
		<xs:sequence>
            <xs:element name="number" type="xs:string" minOccurs="0"/>
			<xs:element name="expirationYearMonth" type="xs:gYearMonth"/>
			<xs:element name="cardholderName" type="xs:string"/>
			<xs:element name="billingAddress" type="com:AddressType"/>
			<xs:element name="siteCard" type="xs:boolean" minOccurs="0"/>
            <xs:element name="paymentToken" type="xs:string"  minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="AX"/>
					<xs:enumeration value="VI"/>
					<xs:enumeration value="CA"/>
					<xs:enumeration value="DI"/>
					<xs:enumeration value="UA"/>
                    <xs:enumeration value="DC"/>
                    <xs:enumeration value="JC"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BaseDebitCardType" abstract="true">
		<xs:sequence>
            <xs:element name="number" type="xs:string" minOccurs="0"/>
			<xs:element name="startYearMonth" type="xs:gYearMonth" minOccurs="0"/>
			<xs:element name="expirationYearMonth" type="xs:gYearMonth"/>
			<xs:element name="cardholderName" type="xs:string"/>
			<xs:element name="billingAddress" type="com:AddressType"/>
            <xs:element name="paymentToken" type="xs:string"  minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="SD"/>
					<xs:enumeration value="VD"/>
					<xs:enumeration value="MD"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
    <xs:complexType name="BaseConfirmationCardType" abstract="true">
        <xs:sequence>
            <xs:element name="cardholderNameLast" type="xs:string"/>
            <xs:element name="cardholderNameFirst" type="xs:string" minOccurs="0"/>
            <xs:element name="cardholderNameMiddle" type="xs:string" minOccurs="0"/>
            <xs:element name="cardNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="paymentToken" type="xs:string" minOccurs="0" />
            <xs:element name="expirationYearMonth" type="xs:gYearMonth" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="selectedConfirmationOptionType">
        <xs:sequence>
          <xs:choice>
            <xs:element name="creditCardOption" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="com:BaseConfirmationCardType"/>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="debitCardOption" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="com:BaseConfirmationCardType">
                            <xs:sequence>
                                <xs:element name="sortCode" type="xs:string" minOccurs="0"/>
                            </xs:sequence>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="loyaltyCardOption" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="program" type="xs:string"/>
                        <xs:element name="identifier" type="xs:string"/>
                        <xs:element name="expirationYearMonthDay" type="xs:date" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="personalIdentificationCardOption" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="cardholderNameLast" type="xs:string"/>
                        <xs:element name="cardholderNameFirst" type="xs:string" minOccurs="0"/>
                        <xs:element name="cardholderNameMiddle" type="xs:string" minOccurs="0"/>
                        <xs:element name="identifier" type="xs:string" minOccurs="0"/>
                        <xs:element name="countryCode" type="xs:string"/>
                        <xs:element name="expirationYearMonthDay" type="xs:date"/>
                        <xs:element name="dateOfBirth" type="xs:date" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="BaseECheckType">
		<xs:sequence>
			<xs:element name="number" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BaseRequestType" abstract="true">
		<xs:sequence>
			<xs:element name="context" type="com:ContextType"/>
			<xs:element minOccurs="0" name="overrides">
				<xs:annotation>
					<xs:documentation>Optional request overrides for testing.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="override">
							<xs:complexType>
								<xs:attribute name="name" use="required"/>
								<xs:attribute name="type" use="required"/>
								<xs:attribute name="value" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="conversationToken" type="xs:string"/>
		<xs:attribute name="version" type="xs:string"/>
		<xs:attribute name="dateTime" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BaseResponseType" abstract="true">
		<xs:sequence>
			<xs:element name="requestStatus" type="com:RequestStatusType"/>
		</xs:sequence>
		<xs:attribute name="conversationToken" type="xs:string"/>
		<xs:attribute name="version" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BookingRecordType">
		<xs:sequence>
            <xs:element name="updateable" type="com:UpdateableBookingType" minOccurs="0"/>
			<xs:element name="bookingDate" type="xs:date"/>
			<xs:element name="departureDate" type="xs:date"/>
			<xs:element name="numberOfOrders" type="xs:int"/>
			<xs:element name="revenueTotal" type="com:MoneyType"/>
			<xs:element name="receiptsTotal" type="com:MoneyType"/>
			<xs:element name="passengers">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="passenger" type="com:PassengerType" maxOccurs="unbounded"
						/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="orders">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="order" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="com:OrderType">
										<xs:sequence>
                                            <xs:element name="updateable" type="com:UpdateableOrderType" minOccurs="0"/>
											<xs:element name="travelSegments" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="travelSegment"
												type="com:TravelSegmentType" minOccurs="0" maxOccurs="unbounded"
												/>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="ticketableFares" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="ticketableFare"
												type="com:TicketableFareType"
												maxOccurs="unbounded"/>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="ticketingOptions" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="ticketingOption"
												type="com:TicketingOptionType"
												maxOccurs="unbounded"/>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="financials" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="totalPrice" type="com:MoneyType"/>
												<xs:element name="basePrice" type="com:MoneyType"
												minOccurs="0"/>
												<xs:element name="upgradePrice"
												type="com:MoneyType" minOccurs="0"/>
												<xs:element name="accommodationPrice"
												type="com:MoneyType" minOccurs="0"/>
												<xs:element name="taxes" type="com:MoneyType"
												minOccurs="0"/>
												<xs:element name="fees" type="com:MoneyType"
												minOccurs="0"/>
												<xs:element name="commissionAmount"
												type="com:MoneyType" minOccurs="0"/>
												</xs:sequence>
												</xs:complexType>
											</xs:element>
                                            <xs:element name="supplyChannelPassengers" minOccurs="0" maxOccurs="1">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="supplyChannelPassengerReference"
                                                                    type="com:SupplyChannelPassengerReferenceType"
                                                                    maxOccurs="unbounded"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="orderNotes" minOccurs="0" maxOccurs="1">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="orderNote"
                                                                    type="com:OrderNoteType"
                                                                    maxOccurs="unbounded"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element name="confirmationInformationRequired" minOccurs="0"
                                                        maxOccurs="1" type="xs:boolean"/>
                                            <xs:element name="paymentCardRequiredForConfirmationInformation"
                                                        minOccurs="0" maxOccurs="1" type="xs:boolean"/>

											<xs:element name="confirmationInformationRequiredOverrides" type="com:ConfirmationInformationRequiredOverrideType" minOccurs="0"/>

                                            <xs:element name="confirmationOptions" minOccurs="0" maxOccurs="1" type="com:ConfirmationOptionsType"/>
                                            <xs:element name="selectedConfirmationOption"
                                                        type="com:selectedConfirmationOptionType"
                                                        minOccurs="0" maxOccurs="1"/>
                                            <xs:element name="commercialAgentName" minOccurs="0" maxOccurs="1" type="xs:string"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="paymentRequirements" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="paymentDue" type="xs:dateTime"/>
						<xs:element name="paymentInformation">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="ANY"/>
									<xs:enumeration value="ORDER_TOTAL"/>
									<xs:enumeration value="BOOKING_TOTAL"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="nextDepositDue" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="minimumDepositAmount" type="com:MoneyType"/>
						<xs:element name="acceptableFormsOfPayment">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="formOfPayment" type="com:FormOfPaymentType"
										maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
                        <xs:element name="refundIsProcessedOnCancel" minOccurs="0" maxOccurs="1" type="xs:boolean"/>
                        <xs:element name="confirmationUponAddPayment" minOccurs="0" maxOccurs="1" type="xs:boolean"/>
                        <xs:element name="confirmationInformationRequired" minOccurs="0" maxOccurs="1" type="xs:boolean"/>
                        <xs:element name="paymentCardRequiredForConfirmationInformation" minOccurs="0" maxOccurs="1" type="xs:boolean"/>
                    </xs:sequence>
				</xs:complexType>
			</xs:element>
            <xs:element name="serviceFeeAllowed" minOccurs="0" type="xs:boolean"/>
			<xs:element name="financials" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="prices" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="price" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="orderIDRef" type="xs:IDREF"
												minOccurs="0"/>
												<xs:element name="amount" type="com:MoneyType"/>
												<xs:element name="postingDate" type="xs:date"
												minOccurs="0"/>
												<xs:element name="commission" minOccurs="0">
												<xs:complexType>
												<xs:sequence>
												<xs:element name="amount" type="com:MoneyType"
												minOccurs="0"/>
												</xs:sequence>
												<xs:attribute name="commissionable"
												type="xs:boolean"/>
												</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="priceID" type="xs:ID" use="required"/>
											<xs:attribute name="type" type="xs:string"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
                        <xs:element name="payments" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="payment" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:complexContent>
                                                <xs:extension base="com:PaymentType">
                                                    <xs:sequence>
                                                        <xs:element name="priceIDRef" type="xs:IDREF" minOccurs="0"/>
                                                        <xs:element name="balanceAmount" type="com:MoneyType" minOccurs="0"/>
                                                        <xs:element name="postingDate" type="xs:date" minOccurs="0"/>
                                                        <xs:element name="receiptNumber" type="xs:string" minOccurs="0"/>
                                                        <xs:element name="referenceReceiptNumber" type="xs:string" minOccurs="0"/>
                                                    </xs:sequence>
                                                </xs:extension>
                                            </xs:complexContent>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="externalIdentification" type="com:ExternalIdentificationType" minOccurs="0"/>
			<xs:element name="customInformation" type="com:CustomInformationType" minOccurs="0"/>
 		</xs:sequence>
		<xs:attribute name="recordLocator" use="required"/>
		<xs:attribute name="status" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="BALANCED"/>
					<xs:enumeration value="CREDIT"/>
					<xs:enumeration value="DEBIT"/>
					<xs:enumeration value="CLOSED"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
    <xs:complexType name="UpdateableBookingType">
        <xs:sequence>
            <xs:element name="passengerIdentity" type="xs:boolean"/>
            <xs:element name="passengerAddress" type="xs:boolean"/>
            <xs:element name="passengerContactInfo" type="xs:boolean"/>
            <xs:element name="loyaltyCard" type="xs:boolean"/>
            <xs:element name="travelDocument" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UpdateableOrderType">
        <xs:sequence>
            <xs:element name="ticketOption" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BookingNoteType">
        <xs:sequence>
            <xs:element name="message" type="xs:string" minOccurs="0"/>
            <xs:element name="createdDateTime" type="xs:dateTime" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="agentId" type="xs:string"/>
        <xs:attribute name="type" type="xs:string"/><!-- AGENT_NOTE -->
    </xs:complexType>
    <xs:complexType name="OrderNoteType">
        <xs:sequence>
            <xs:element name="message" type="xs:string" minOccurs="0"/>
            <xs:element name="createdDateTime" type="xs:dateTime" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="type" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="AddedOrderNoteType">
        <xs:complexContent>
            <xs:extension base="com:OrderNoteType">
                <xs:attribute name="orderID" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
	<xs:simpleType name="CabinClassType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="CancellationReasonType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="DISRUPTION"/>
			<xs:enumeration value="FULFILLMENT_FAILURE"/>
			<xs:enumeration value="FULL_TICKET_NOT_REQD"/>
			<xs:enumeration value="NO_UPGRADE_AVAILABLE"/>
			<xs:enumeration value="NO_RESERVATION_AVAILABLE"/>
			<xs:enumeration value="PARTIAL_TICKET_NOT_REQD"/>
			<xs:enumeration value="OTHER"/>
		</xs:restriction>
	</xs:simpleType>
    <xs:simpleType name="RefundTargetType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="FORM_OF_PAYMENT"/>
            <xs:enumeration value="VOUCHER"/>
            <xs:enumeration value="SPLIT_FORM_OF_PAYMENT_VOUCHER"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="BaseCancellationOptionType" abstract="true">
        <xs:sequence>
            <xs:element name="refundTarget" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>These two money elements will appear only when the refundTarget contains a voucher.</xs:documentation>
                        </xs:annotation>
                        <xs:element name="voucherAmount" type="com:MoneyType" minOccurs="1"/>
                        <xs:element name="voucherValue" type="com:MoneyType" minOccurs="1"/>
                    </xs:sequence>
                    <xs:attribute name="type" type="com:RefundTargetType" use="required"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SelectedCancellationOptionType">
        <xs:complexContent>
            <xs:extension base="com:BaseCancellationOptionType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CancellationOptionType">
        <xs:complexContent>
            <xs:extension base="com:BaseCancellationOptionType">
                <xs:sequence>
                                	<xs:element name="penalty" minOccurs="0" maxOccurs="1">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="total" type="com:MoneyType" minOccurs="1" maxOccurs="1"/>
												<xs:element name="breakdown" minOccurs="0" maxOccurs="1">
													<xs:complexType>
														<xs:sequence>
															<xs:element name="component" minOccurs="0" maxOccurs="unbounded">
																<xs:complexType>
																	<xs:simpleContent>
																		<xs:extension base="com:MoneyType">
																			<xs:attribute name="type" use="required">
																				<xs:simpleType>
																					<xs:restriction base="xs:string">
																						<xs:enumeration value="TICKET"/>
																						<xs:enumeration value="RESERVATION"/>
																						<xs:enumeration value="ACCOMMODATION"/>
                                                                                        <xs:enumeration value="FEE"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:attribute>
																		</xs:extension>
																	</xs:simpleContent>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
                                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
                            </xs:complexType>

    <xs:complexType name="CancellationSummaryType">
        <xs:sequence>
            <xs:element name="cancellationOptions" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="cancellationOption" minOccurs="1" maxOccurs="unbounded"
                                    type="com:CancellationOptionType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
			<xs:element name="priceReversals" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
                        <xs:element name="total" type="com:MoneyType" minOccurs="1" maxOccurs="1"/>
                        <xs:element name="breakdown" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="component" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="amount" type="com:MoneyType"/>
                                            </xs:sequence>
                                            <xs:attribute name="type" type="xs:string"/>
                                        </xs:complexType>
